name: ReLens
desc: 解锁会员

http:
  mitm:
    - "buy.itunes.apple.com"
  script:
    - match: ^https?:\/\/buy\.itunes\.apple\.com\/verifyReceipt$
      name: ReLens
      type: response
      require-body: true
      timeout: 30
      max-size: 0
script-providers:
  ReLens:
    url: https://raw.githubusercontent.com/chxm1023/Rewrite/main/ReLens.js
    interval: 86400    
