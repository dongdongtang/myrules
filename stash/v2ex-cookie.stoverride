name: v2ex获取cookie
desc: 请查看覆写内容获取cookie.


http:
  mitm:
    - "*.v2ex.com"
    
  # 获取v2ex Cookie
  script:
    - match: ^https:\/\/www\.v2ex\.com\/mission\/daily
      name: v2ex-cookie
      type: request
      require-body: true
      timeout: 30

script-providers:
  v2ex-cookie:
    url: https:https://raw.githubusercontent.com/chavyleung/scripts/master/v2ex/v2ex.cookie.js
    interval: 86400
