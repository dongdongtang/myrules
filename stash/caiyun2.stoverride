name: 彩云天气解锁
desc: 彩云天气解锁

http:
  mitm:
    - "biz.caiyunapp.com"
    - "biz.cyapi.cn"
  script:
    - match: ^https:\/\/biz\.(caiyunapp\.com|cyapi\.cn)\/v2\/user\?app_name\=weather
      name: cytq
      type: response
      require-body: true
      timeout: 30
      max-size: 0
script-providers:
  cytq:
    url: https://raw.githubusercontent.com/Maasea/surgeModule/master/Script/Weather/weather.js
    interval: 86400    
