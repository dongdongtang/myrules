name: v2ex签到
desc: 签到脚本将在每天的凌晨0:10执行


http:
  mitm:
    - "*.v2ex.com"

  # 获取v2ex Cookie
  script:
    - match: ^https:\/\/www\.v2ex\.com\/mission\/daily
      name: v2excheckin 
      type: request
      require-body: true
      timeout: 30
   
cron:
  # 签到
  script:
    - name: v2excheckin
      cron: '10 0 0 * * *'
      timeout: 60
 
script-providers:
  v2excheckin:
    url: https: https://raw.githubusercontent.com/chavyleung/scripts/master/v2ex/v2ex.js
    interval: 86400
